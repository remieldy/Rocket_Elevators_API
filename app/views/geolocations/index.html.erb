               <section>
                   <div class="card z-depth-3">
                       
                       <!-- OWL SLIDER -->
                       <div class="card-content">

                           <div id="map"></div>
                              <script>

                              var adresses_list = [];
                              
                                <% @markers.each do |adresse| %>
                                    adresses_list.push({address: "<%= adresse[:address] %>",
                                    lat: <%= adresse[:lat] %>,
                                    lng: <%= adresse[:lng] %>,
                                    customer: "<%= adresse[:name] %>", 
                                    batteries: "<%= adresse[:batteries] %>",
                                    columns: "<%= adresse[:columns] %> ",
                                    elevators: "<%= adresse[:elevators] %> ",
                                    contact: "<%= adresse[:tech_name] %> ", 
                                    floor_number: "<%= adresse[:floor_number] %> "}
                                    );
                                <% end %> 
                                   

                                   function initMap() {
                                   
                                   var quebec = {lat: 46.8139, lng: -71.2080};
                                   
                                   var map = new google.maps.Map(
                                       document.getElementById('map'), {zoom: 8, center: quebec});

                                       var locations = adresses_list.map(function(adresse, i){
                                           var geo = new google.maps.Marker({
                                               position: adresse,
                                               map: map
                                           })
                                           var infoWindow = new google.maps.InfoWindow({
                                            maxWidth: 300
                                            });
                                            
                                            var window = '<h5>' + adresse.address + '</h5>' +
                                                'Customer: ' + adresse.customer + '</br>' + 
                                                '<br>Contact: ' + adresse.contact + '</br>' +
                                                '<br>Columns: ' + adresse.columns + '</br>' + 
                                                '<br>Elevators: ' + adresse.elevators + '</br>' + 
                                                '<br>Batteries: ' + adresse.batteries + '</br>' +
                                                '<br> Floor Number: ' + adresse.floor_number;
    
                                            geo.addListener('click', function() {
                                                infoWindow.setContent(window)
                                                infoWindow.open(map, geo);
                                            
                                              });
                                            
                                              return geo

                                       })
                                   }

                               </script>
                               <script async defer
                                       src="https://maps.googleapis.com/maps/api/js?key=<%=ENV['GOOGLE_API']%>&callback=initMap">
                               </script>
                       </div>
                       <!-- /OWL SLIDER -->
                       
